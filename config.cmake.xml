<?xml version="1.0" encoding="UTF-8"?>
	<Installer>
	<Name>Fugio</Name>
	<Version>@FUGIO_VERSION@</Version>
	<Title>Fugio Installer</Title>
	<Publisher>www.bigfug.com</Publisher>
	<StartMenuDir>Fugio</StartMenuDir>
	<TargetDir>\\@ApplicationsDir\\@/@@Fugio@@</TargetDir>
	<RemoteRepositories>
		<Repository>
		   <Url>@FUGIO_REPOSITORY_URL@</Url>
		</Repository>
	</RemoteRepositories>
</Installer>
